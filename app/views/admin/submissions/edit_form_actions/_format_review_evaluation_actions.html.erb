<%= f.button :submit, name: 'approved', class: "btn btn-success", value: "Format Review Completed", title: "Proceed to '#{t("#{current_partner.id}.admin_filters.format_review_completed.title")}'", data: {toggle:"tooltip", placement:"top" } %>
<%= f.button :submit, name: 'rejected', class: "btn btn-danger", value: "Reject & request revisions", title: "Return to '#{t("#{current_partner.id}.admin_filters.format_review_incomplete.title")}'", data: {toggle:"tooltip", placement:"top" } %>
<%= f.button :submit, name: 'update_format_review', class: "btn btn-info", value: "Update Metadata Only", title: "Remain at '#{t("#{current_partner.id}.admin_filters.format_review_submitted.title")}'", data: {toggle:"tooltip", placement:"top" } %>

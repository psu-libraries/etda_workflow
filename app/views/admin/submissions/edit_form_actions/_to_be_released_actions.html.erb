<%= f.button :submit, name: 'update', class: "btn btn-info", value: "Update Metadata Only", title: "Remain at '#{t("#{current_partner.id}.admin_filters.final_submission_approved.title")}'", data: {toggle:"tooltip", placement:"top" } %>
<%= f.button :submit, name: 'rejected', class: "btn btn-danger", value: "Remove from Submission to be Released", id: 'rejected', title: "Return to '#{t("#{current_partner.id}.admin_filters.final_submission_submitted.title")}'", data: {toggle:"tooltip", placement:"top" } %>
<%= f.button :submit, name: 'send_to_hold', class: "btn btn-warning", value: "Place this Submission On Hold", id: 'send_to_hold', title: "Move to '#{t("#{current_partner.id}.admin_filters.final_submission_on_hold.title")}'", data: {toggle:"tooltip", placement:"top" } %>

<div class="row">
  <div class="col-xs-12">
    <h1><%= @view.title %></h1>
  </div>
</div>

<div class="row">
  <div class="col-xs-12">
    <% @view.filters.each do |filter| %>
      <% next if filter[:title].nil? %>
      <%= render partial: 'filter', locals: { filter: filter } %>
    <% end %>
  </div>
</div>

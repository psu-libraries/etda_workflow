<fieldset>
  <legend><strong>Document Availability</strong><hr/></legend>
  <% if current_partner.sset? %>
    <div class="alert alert-info">
      <p class="hint"><u>At this time submissions are not being made public.</u>  If they are in the future, please select the access level for your submission.</p>
    </div>
  <% end %>
    <div class="author">
      <%= f.input :access_level,
                  as: :radio_buttons,
                  label: "Access Level",
                  required: true,
                  collection: AccessLevel.display.map{ |k| ["<strong>#{k[:label]}</strong> -- #{k[:description]}".html_safe,
                  k[:type], { class: "#{k[:type]}" }]},
                  wrapper: :vertical_radio_and_checkboxes %>
    </div>
</fieldset>
<%= render partial: 'invention_disclosure_information', locals: {f: f} %>



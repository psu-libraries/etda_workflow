<% unless current_partner.graduate? %>
<div class="col-xs-12">
  <%= render 'new_submission_button' %>
</div>
<% end %>

<div class="row">
  <div class="col-xs-12">
    <h2>My Submissions</h2>
    <p class="row">
      <%= render partial: 'admin_submission_buttons' if admin? && current_partner.graduate? && !(ENV['HOSTNAME'] == 'etdaworkflow1prod') %>
    </p>
  </div>
</div>

<div id="submissions" class="row">

  <div class="col-xs-12 submission">
    <br />
    <div class="alert alert-info">
    <p class="hint"><%= @view.no_submissions_message.html_safe %></p>
    </div>

    <%= render 'progress_indicator_preview'%>
  </div>

</div>

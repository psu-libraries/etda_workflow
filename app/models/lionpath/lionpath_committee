class Lionpath::LionpathCommittee
  include Lionpath::LionpathCommitteeFactory

  def import(row)
    author = Author.find_by(psu_idn: row['Student ID'])
    submission = author.submissions.find{ |s| s.degree.degree_type.slug == 'dissertation' }
    factory = LionpathCommitteeFactory.new(row, submission)
    factory.create_member
  end
end
